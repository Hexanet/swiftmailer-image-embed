{
  "name": "hexanet/swiftmailer-image-embed",
  "type": "library",
  "description": "Swiftmailer plugin to automatically embed images into message",
  "keywords": [ "swiftmailer", "image" ],
  "homepage": "https://github.com/Hexanet/swiftmailer-image-embed",
  "license": "MIT",
  "authors": [
    { "name": "Nicolas Hart", "email": "nhart@hexanet.fr" }
  ],
  "require": {
    "php": ">=7.0.0",
    "swiftmailer/swiftmailer": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "suggests": {
    "openbuildings/swiftmailer-css-inliner": "Inline css in your Swiftmailer messages"
  },
  "autoload": {
    "psr-4": { "Hexanet\\Swiftmailer\\": "src" }
  },
  "autoload-dev": {
  }
}
